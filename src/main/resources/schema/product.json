{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "Product",
  "type": "object",
  "properties": {
    "catalogId": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "price": {
      "type": "number"
    },
    "quantity": {
      "type": "integer",
      "minimum": 1
    }
  },
  "required": ["catalogId", "price", "name", "quantity", "description"]
}
